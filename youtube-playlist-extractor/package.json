{
  "name": "youtube-playlist-extractor",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "playlist-extractor": "./dist/bin/playlist-extractor.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/playlist-extractor.js",
    "dev": "ts-node-esm bin/playlist-extractor.ts",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fs": "0.0.1-security",
    "ora": "^7.0.1",
    "path": "^0.12.7",
    "yargs": "^17.7.2",
    "youtube-dl-exec": "^2.4.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}